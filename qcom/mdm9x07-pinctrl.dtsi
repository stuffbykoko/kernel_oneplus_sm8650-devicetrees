					drive-strength = <2>;
					bias-disable;
				};
			};

			blsp1_uart5_console_rx5_sleep: blsp1_uart5_console_rx5_sleep {
				mux {
					pins = "gpio9";
					function = "blsp_uart_rx5";
				};

				config {
					pins = "gpio9";
					drive-strength = <2>;
					bias-no-pull;
				};
			};
		};

